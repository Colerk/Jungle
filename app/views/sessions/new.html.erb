<h1>Login</h1>

<%= form_tag '/login' do %>
<div class="form-group">
<label class="col-sm-1 col-form-label">Email: </label>
<%= text_field_tag :email %>
  </div>
  <div class="form-group">
<label class="col-sm-1 col-form-label">Password: </label>
 <%= password_field_tag :password %>
  </div>
  <%= submit_tag "Submit" %>

<% end %>